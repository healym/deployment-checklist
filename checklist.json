{
    "name": "Batch Job Checklist",
    "list_items": [
        {
        "prompt": "Have all walkthrought, peer reviews, and design reviews been performed?",
        "default_choice": "Y",
        "choices": {
            "Y": "DONE",
            "N": "COMMENT"
        }
    },
    {
        "prompt": "If a new script, has the driver script been committed?",
        "default_choice": "Y",
        "choices": {
            "Y": "DONE",
            "N": "COMMENT"
        }
    },
    {
        "prompt": "Has Failure Mode Analysis been conducted and bench results are verified for system impact?",
        "default_choice": "Y",
        "choices": {
            "Y": "DONE",
            "N": "COMMENT"
        }
    },
    {
        "prompt": "Does the job have a single point of failure?",
        "default_choice": "N",
        "choices": {
            "N": "DONE",
            "Y": "COMMENT"
        }
    },
    {
        "prompt": "Has impact analysis and remediation steps been documented for any external resources?",
